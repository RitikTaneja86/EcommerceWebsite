<section id="members-list">
    <div class="member-head" data-aos="zoom-in"  data-aos-offset="125" data-aos-delay="100"  data-aos-duration="1000">
        <h1> Our Bakery Chef's</h1>
    </div>
    <div class="member-table" data-aos="zoom-in"  data-aos-offset="125" data-aos-delay="100"  data-aos-duration="1000">
        <% profiles.forEach(function(profile){%>
            <div class="member">
                <div class="member-pic">
                    <img src="<%=profile.profileimage%>" alt="member-pic">
                </div>
                <div class="member-info">
                    <div class="name">
                        <h2><a href="/member-profile/<%=profile._id%>"><%=profile.firstname %> </a></h2>
                    </div>
                    <div class="experience">
                        <h3><%=profile.bakeryname%> </h3>
                    </div>
                    <div class="specialisation">
                        <h3><%=profile.description%> </h3>
                    </div>
                </div>
            </div>
        <%})%>
    </div>
</section>

<section id="place-order">
    <h1 class="place-order-h1" data-aos="zoom-in"  data-aos-offset="125" data-aos-delay="100"  data-aos-duration="1000">STEPS TO PLACE ORDER</h1>
    
    <div class="table" data-aos="zoom-in"  data-aos-offset="125" data-aos-delay="100"  data-aos-duration="1000">
        <div class="service-box">
            <div class="table-h1">
                <h1>Step 1</h1>
            </div>
            <div class="icon">
                <i class="fas fa-birthday-cake"></i>
            </div> 
            <div class="service-h2">
                <h2>Select Your Cake</h2>
            </div>
            <div class="service-p">
                <p>Choose the cake of your choice from the whole lot menu</p>
            </div>
            <div id="step-1" class="next-step">
                <i class="fas fa-arrow-circle-right"></i>
            </div>
        </div>

        <div class="service-box">
            <div class="table-h1">
                <h1>Step 2</h1>
            </div>
            <div class="icon">
                <i class="fas fa-user"></i>
            </div>
            <div class="service-h2">
                <h2>Go To Bakers Profile</h2>
            </div>
            <div class="service-p">
                <p>Profile will have the contact info of the bakery</p>
            </div>
            <div id="step-2" class="next-step">
                <i class="fas fa-arrow-circle-right"></i>
            </div>
        </div>
            
        <div class="service-box">
            <div class="table-h1">
                <h1>Step 3</h1>
            </div>
            <div class="icon">
                <i class="fas fa-address-book"></i>
            </div>
            <div class="service-h2">
                <h2>Place Your Order</h2>
            </div>
            <div class="service-p">
                <p>You will recieve the confirmation from the bakery</p>
            </div>
            <div id="step-3" class="next-step">
                <i class="fas fa-arrow-circle-right"></i>
            </div>
        </div>
            
        <div id="service-box-4" class="service-box">
            <div class="table-h1">
                <h1>Step 4</h1>
            </div>
            <div class="icon">
                <i class="fas fa-stopwatch"></i>
            </div>
            <div class="service-h2">
                <h2>24 Hours Delivery</h2>
            </div>
            <div class="service-p">
                <p>Your order will be delivered within 24 hours</p>
            </div>
        </div>
    </div>
</section>


