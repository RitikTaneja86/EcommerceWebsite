<section id="carousel">
    <div id="carouselExampleIndicators" class="carousel slide slideshow" data-ride="carousel" data-interval="3500">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img  src="../images/cake1.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img  src="../images/cake2.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img  src="../images/cake3.jpg" alt="Third slide">
            </div>
        </div>
    </div>

    <div class="carousel-text">
        <div class="heading">
            <h2>explore wide range of yummilicious cakes and cookies</h2>
        </div>
        <div class="post-script">
            <p>freshly baked with love from our kitchen</p>
        </div>
        <div class="shop-button">
            <a href="/shop">SHOP NOW</a>
            <i class="fas fa-arrow-circle-right"></i>
        </div>
        </div>
    </div>
</section>

